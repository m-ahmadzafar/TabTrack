{
  "manifest_version": 3,
  "name": "TabTrack",
  "version": "1.0",
  "icons": {
    "16": "logo2.png",
    "48": "logo2.png",
    "128": "logo2.png"
  },
  "description": "TabTrack groups tabs by website type and provide a visual overview for convenience.",
  "permissions": [
    "tabs",
    "storage",
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "logo2.png",
      "48": "logo2.png",
      "128": "logo2.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}


